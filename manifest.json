{
  "name": "Repo Link",
  "description": "Make your life easier with direct link to the repository github page",
  "version": "1.2",
  "manifest_version": 3,
  "icons": {
    "48": "icons/repolink-48.png",
    "96": "icons/repolink-96.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["main.js"]
    }
  ]
}